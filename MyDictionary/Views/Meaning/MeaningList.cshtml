@model List<DictionaryDto.MeaningDto>

@{ 
    if (Model.Count() > 0)
    {
        <ul class="list-group">
            @for (int index = 0; index < Model.Count(); index++)
            {
                <li class="list-group-item">
                    @Model.ElementAt(index).MeaningText
                </li>
                if (Model.ElementAt(index).ExampleList.Count() > 0)
                {
                    <li>
                        @{ Html.RenderPartial("~/Views/Example/ExampleList.cshtml", Model.ElementAt(index).ExampleList); }
                    </li>
                }
            }
        </ul>   
    }
}
